{
  "name": "google-teams",
  "version": "1.2.8",
  "description": "An unofficial desktop app for Google Teams",
  "main": "./src/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --mac --publish=never"
  },
  "repository": "https://github.com/rimonhanna/GoogleTeams",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Rimon Hanna",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.4.0",
    "electron-builder": "^22.8.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "electron-window-state": "^5.0.3",
    "mac-screen-capture-permissions": "^1.1.0"
  },
  "build": {
    "appId": "com.rimonhanna.googleTeams",
    "productName": "Google Teams",
    "mac": {
      "target": "zip",
      "extendInfo": {
        "NSCameraUsageDescription": "This app requires camera access for video call in Google Meet.",
        "NSMicrophoneUsageDescription": "This app requires microphone access for audio call in Google Meet."
      }
    }
  }
}
